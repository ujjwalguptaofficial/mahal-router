<script>
import { Component } from "mahal";
import { BaseComponent } from "./base";

export default class extends BaseComponent {
    get id() {
        return this.route.param.id;
    }

    buy() {
        this.router.goto({
            name: "buy-project",
        });
    }
}
</script>

